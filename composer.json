{
    "name": "getherbie/website",
    "homepage": "http://www.getherbie.org",
    "description": "The content for the Herbie website. Herbie is a flat-file CMS & blog system.",
    "type": "project",
    "minimum-stability": "dev",
    "license":"BSD-3-Clause",
    "require": {
        "php": ">=5.4.0",
        "getherbie/herbie": "dev-master",
        "getherbie/composer": "dev-master",
        "getherbie/plugin-adminpanel": "dev-master",
        "getherbie/plugin-disqus": "dev-master",
        "getherbie/plugin-feed": "dev-master",
        "getherbie/plugin-gist": "dev-master",
        "getherbie/plugin-googlemaps": "dev-master",
        "getherbie/plugin-grid": "dev-master",
        "getherbie/plugin-highlight": "dev-master",
        "getherbie/plugin-highlightjs": "dev-master",
        "getherbie/plugin-imagine": "dev-master",
        "getherbie/plugin-lipsum": "dev-master",
        "getherbie/plugin-mathjax": "dev-master",
        "getherbie/plugin-menu": "dev-master",
        "getherbie/plugin-random": "dev-master",
        "getherbie/plugin-redirect": "dev-master",
        "getherbie/plugin-simplecontact": "dev-master",
        "getherbie/plugin-simplesearch": "dev-master",
        "getherbie/plugin-smartypants": "dev-master",
        "getherbie/plugin-test": "dev-master",
        "getherbie/plugin-video": "dev-master",
        "getherbie/plugin-xmlsitemap": "dev-master"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "Herbie\\Composer\\Installer::setPermission"
        ]
    },
    "extra": {
        "writable": [
            "site/cache/data",
            "site/cache/page",
            "site/cache/twig"
        ],
        "executable": []
    },
    "autoload": {
        "psr-4": {
            "Herbie\\": "/web/www/getherbie/herbie/system/",
            "Herbie\\Composer\\": "/web/www/getherbie/composer/"
        }
    }
}
