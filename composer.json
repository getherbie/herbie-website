{
    "name": "getherbie/website",
    "homepage": "http://www.getherbie.org",
    "description": "The content for the Herbie website. Herbie is a file based CMS & Blos system and not ready for production use yet.",
    "type": "project",
    "minimum-stability": "dev",
    "license":"BSD-3-Clause",
    "require": {
        "php": ">=5.4.0",
        "getherbie/herbie": "dev-master",
        "getherbie/composer": "dev-master",
        "getherbie/plugin-disqus": "dev-master",
        "getherbie/plugin-googlemaps": "dev-master",
        "getherbie/plugin-highlight": "dev-master",
        "getherbie/plugin-imagine": "dev-master",
        "getherbie/plugin-lipsum": "dev-master",
        "getherbie/plugin-shortcode": "dev-master",
        "getherbie/plugin-test": "dev-master",
        "getherbie/plugin-vimeo": "dev-master",
        "getherbie/plugin-youtube": "dev-master"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "Herbie\\Composer\\Installer::setPermission"
        ]
    },
    "extra": {
        "writable": [
            "site/cache/data",
            "site/cache/page",
            "site/cache/twig"
        ],
        "executable": []
    },
    "autoload": {
        "psr-4": {
            "Herbie\\": "/web/www/getherbie/herbie/src/",
            "herbie\\plugin\\widgets\\": "/web/www/getherbie/plugins/widgets/",
            "herbie\\plugin\\test\\": "/web/www/getherbie/plugins/test/",
            "herbie\\plugin\\lipsum\\": "/web/www/getherbie/plugins/lipsum/",
            "herbie\\plugin\\vimeo\\": "/web/www/getherbie/plugins/vimeo/",
            "herbie\\plugin\\youtube\\": "/web/www/getherbie/plugins/youtube/",
            "herbie\\plugin\\googlemaps\\": "/web/www/getherbie/plugins/googlemaps/",
            "herbie\\plugin\\disqus\\": "/web/www/getherbie/plugins/disqus/",
            "herbie\\plugin\\highlight\\": "/web/www/getherbie/plugins/highlight/",
            "herbie\\plugin\\shortcode\\": "/web/www/getherbie/plugins/shortcode/",
            "herbie\\plugin\\imagine\\": "/web/www/getherbie/plugins/imagine/",
            "herbie\\plugin\\feediting\\": "/web/www/getherbie/plugins/feediting/"
        }
    }
}
